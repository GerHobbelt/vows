{
  "name": "perjury",
  "version": "1.0.1",
  "license": "Apache-2.0",
  "description": "testing framework with similar structure to vows.js",
  "keywords": [
    "unit test",
    "vows",
    "test"
  ],
  "author": {
    "name": "Evan Prodromou",
    "email": "evan@fuzzy.ai"
  },
  "main": "lib/index.js",
  "bin": "./bin/perjury.js",
  "scripts": {
    "lint": "node_modules/.bin/jshint bin/*.js lib/*.js test/*.js",
    "test": "node test/standalone.js && node bin/perjury.js test/test-*.* && (check-node-version --node '< 7.10.1' --quiet || node bin/perjury.js test/atest-*.js)"
  },
  "directories": {
    "lib": "./lib"
  },
  "repository": "fuzzy-ai/perjury",
  "dependencies": {
    "async": "^2.0.1",
    "coffee-script": "^1.10.0",
    "debug": "^3.1.0",
    "lodash": "^4.17.4",
    "yargs": "^10.0.3"
  },
  "devDependencies": {
    "check-node-version": "^2.1.0",
    "jshint": "^2.9.3",
    "util-promisify": "^2.1.0"
  }
}
